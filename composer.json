{
  "name": "paxha/laravel-loggable",
  "description": "A simple loggable package for laravel",
  "type": "library",
  "require": {
    "php": "^7.2|^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3",
    "orchestra/testbench": "^v6.18"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Pasha",
      "email": "e.hassanrazapasha@gmail.com"
    },
    {
      "name": "Shehroz Afzal",
      "email": "shehrozafzal98@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Loggable\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Loggable\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Loggable\\LoggableServiceProvider",
        "Loggable\\LoggableEventServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev"
}
